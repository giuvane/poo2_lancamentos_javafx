<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="153.0" prefWidth="357.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.utfpr.giuvane.controle.FXMLCadastrosCategoriasDialogController">
   <children>
      <GridPane layoutX="12.0" layoutY="14.0" prefHeight="48.0" prefWidth="333.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="159.0" minWidth="10.0" prefWidth="118.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="215.0" minWidth="10.0" prefWidth="215.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="TOP_LEFT" text="Descrição:" />
            <TextField fx:id="txtCategoriaDescricao" alignment="TOP_LEFT" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Group>
         <children>
            <Button fx:id="btnConfirmar" layoutX="152.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleButtonConfirmar" text="Confirmar">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icones/add.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btnCancelar" layoutX="252.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleButtonCancelar" prefHeight="25.0" prefWidth="95.0" text="Cancelar">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icones/cancel.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </Group>
   </children>
</AnchorPane>
